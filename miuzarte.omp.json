{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "gitOrange": "#f34f29",
    "goBlue": "#00add8",
    "javaBlue": "#5382A1",
    "javaOrange": "#FFA518",
    "luaBlue": "#00007f",
    "nodeBlack": "#333333",
    "nodeGreen": "#339933",
    "phpBlack": "#000000",
    "phpBlue": "#777BB3",
    "pythonBlue": "#4584b6",
    "pythonYellow": "#ffde57",
    "winBlue": "#0078d4",
    "winGray": "#3f3f3f",
    "winWhite": "#ededed"
  },
  "secondary_prompt": {
    "template": "    | ",
    "foreground": "p:winBlue"
  },
  "accent_color": "#ffffff",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": "{{if .WSL}} WSL at {{end}} {{.Icon}} ",
          "foreground": "p:winWhite",
          "background": "p:winBlue",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": " {{.Path}} ",
          "foreground": "p:winGray",
          "powerline_symbol": "\ue0b0",
          "background": "p:winWhite",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_gone_icon": "\ueab8",
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_max_length": 16,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "cli"
          },
          "leading_diamond": " \ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{.UpstreamIcon}} {{.HEAD}}{{if .BranchStatus}} {{.BranchStatus}}{{end}}{{if .Working.Changed}} \ue0b1 \uf044 {{.Working.String}}{{end}}{{if .Staging.Changed}} \ue0b1 \uf046 {{.Staging.String}}{{end}}{{if gt .StashCount 0}} \ue0b1 \ueb4b {{.StashCount}}{{end}} ",
          "foreground": "p:winWhite",
          "background": "p:gitOrange",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{if or (.Working.Changed) (.Staging.Changed)}}#a97400{{end}}",
            "{{if and (gt .Ahead 0) (gt .Behind 0)}}#54433a{{end}}",
            "{{if gt .Ahead 0}}#744d89{{end}}",
            "{{if gt .Behind 0}}#744d89{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} ",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "type": "project",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{if .Error}}{{.Error}}{{else}}{{.Full}}{{end}}",
          "foreground": "p:goBlue",
          "background": "p:winWhite",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue235 {{if .Error}}{{.Error}}{{else}}{{if .Venv}}{{.Venv}} {{end}}{{.Full}}{{end}}",
          "foreground": "p:pythonYellow",
          "background": "p:pythonBlue",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue738 {{ .Full }}",
          "foreground": "p:winWhite",
          "background": "p:javaBlue",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue620 5.4.2",
          "foreground": "p:winWhite",
          "background": "p:luaBlue",
          "type": "lua",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": "<#cc3a3a>\ue5fa</>",
            "yarn_icon": "<#348cba>\ue6a7</>"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{if .PackageManagerIcon}}{{.PackageManagerIcon}} {{end}}{{.Full}}",
          "foreground": "p:nodeGreen",
          "background": "p:nodeBlack",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue73d {{ .Full }}",
          "foreground": "p:phpBlue",
          "background": "p:phpBlack",
          "type": "php",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none"
          },
          "template": " \udb80\ude06 <f>{{.String}}</f> ",
          "foreground": "p:winWhite",
          "powerline_symbol": "\ue0b2",
          "background": "p:winBlue",
          "type": "status",
          "style": "accordion",
          "invert_powerline": true
        },
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none",
            "style": "austin"
          },
          "template": " \ueba2 {{.FormattedMs}} ",
          "foreground": "p:winGray",
          "powerline_symbol": "\ue0b2",
          "background": "p:winWhite",
          "type": "executiontime",
          "style": "accordion",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \uf073 {{.CurrentDate | date .Format}} ",
          "foreground": "p:winWhite",
          "background": "p:winBlue",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500\ue0b3 {{if .Root}}#{{else}}${{end}} ",
          "foreground": "p:winBlue",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.UserName}}@{{.HostName}}",
          "foreground": "accent",
          "type": "session",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}